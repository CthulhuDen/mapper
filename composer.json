{
    "name": "tarantool/mapper",
    "description": "PHP Object Mapper for Tarantool.",
    "keywords": ["tarantool", "client", "pure", "nosql", "orm"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^5.5|^7.0",
        "tarantool/client": "dev-master",
        "rybakit/msgpack": "dev-master"
    },
    "required-dev": {
        "phpunit/phpunit": "^4.8"
    },
    "autoload-dev": {
        "classmap": [
            "tests"
        ]
    },
    "autoload": {
        "psr-4": {
            "Tarantool\\Mapper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dmitry Krokhin",
            "email": "nekufa@gmail.com"
        }
    ],
    "scripts": {
        "cs": "php-cs-fixer fix . --level=psr2"
    }
}
