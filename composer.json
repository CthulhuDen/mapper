{
    "name": "tarantool/mapper",
    "description": "PHP Object Mapper for Tarantool.",
    "keywords": ["tarantool", "client", "pure", "nosql", "mapper"],
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.0",
        "tarantool/client": "^0.2.0"
    },
    "suggest": {
        "ext-msgpack": "For using Pecl- or PeclLitePacker.",
        "rybakit/msgpack": "For using PurePacker."
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0",
        "rybakit/msgpack": "^0.2.1"
    },
    "autoload-dev": {
        "classmap": ["tests"]
    },
    "autoload": {
        "psr-4": {
            "Tarantool\\Mapper\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Dmitry Krokhin",
            "email": "nekufa@gmail.com"
        }
    ]
}
